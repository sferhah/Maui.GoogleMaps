<?xml version="1.0" encoding="UTF-8"?>

<ContentPage
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:maps="clr-namespace:Maui.GoogleMaps;assembly=Maui.GoogleMaps"
    x:Class="MauiGoogleMapSample.ClusteringPage"
    Title="Pins">

    <ContentPage.Content>

        <Grid VerticalOptions="FillAndExpand" HorizontalOptions="FillAndExpand">
            <maps:Map x:Name="map"
                      Grid.Row="1"
                      VerticalOptions="FillAndExpand" />

            <Button x:Name="buttonAddPins" Text="Add Pins" HorizontalOptions="FillAndExpand" VerticalOptions="Start" />
        </Grid>

    </ContentPage.Content>
</ContentPage>